<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.greenacademy.ga_finalprojecthm.fragment.ContactFragment">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_bottom_general"
        android:paddingBottom="@dimen/padding_bottom_general"
        android:paddingEnd="@dimen/padding_end_general"
        android:paddingLeft="@dimen/padding_start_general"
        android:paddingRight="@dimen/padding_end_general"
        android:paddingStart="@dimen/padding_start_general"
        android:paddingTop="@dimen/padding_top_general"
        android:background="#ff000000"
        android:gravity="center_vertical"
        android:text="@string/contact"
        android:textAllCaps="true"
        android:textColor="#ffffffff"
        android:textSize="@dimen/font_size_general"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_bottom_general"
        android:layout_marginEnd="@dimen/margin_end_general"
        android:layout_marginLeft="@dimen/margin_start_general"
        android:layout_marginRight="@dimen/margin_end_general"
        android:layout_marginStart="@dimen/margin_start_general"
        android:layout_marginTop="@dimen/margin_top_general"
        android:paddingBottom="@dimen/padding_bottom_general"
        android:paddingEnd="@dimen/padding_end_general"
        android:paddingLeft="@dimen/padding_start_general"
        android:paddingRight="@dimen/padding_end_general"
        android:paddingStart="@dimen/padding_start_general"
        android:paddingTop="@dimen/padding_top_general"
        android:gravity="center_vertical"
        android:text="@string/contact_info"
        android:textAllCaps="true"
        android:textSize="@dimen/font_size_general"
        android:textColor="#ff000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvCustomerService"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_bottom_general"
        android:layout_marginEnd="@dimen/margin_end_general"
        android:layout_marginLeft="@dimen/margin_start_general"
        android:layout_marginRight="@dimen/margin_end_general"
        android:layout_marginStart="@dimen/margin_start_general"
        android:layout_marginTop="@dimen/margin_top_general"
        android:paddingBottom="@dimen/padding_bottom_general"
        android:paddingEnd="@dimen/padding_end_general"
        android:paddingLeft="@dimen/padding_start_general"
        android:paddingRight="@dimen/padding_end_general"
        android:paddingStart="@dimen/padding_start_general"
        android:paddingTop="@dimen/padding_top_general"
        android:textColor="#ff000000"
        android:text="@string/customer_service"
        android:textSize="@dimen/font_size_general"
        android:gravity="center_vertical"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_bottom_general"
        android:layout_marginEnd="@dimen/margin_end_general"
        android:layout_marginLeft="@dimen/margin_start_general"
        android:layout_marginRight="@dimen/margin_end_general"
        android:layout_marginStart="@dimen/margin_start_general"
        android:layout_marginTop="@dimen/margin_top_general"
        android:paddingBottom="@dimen/padding_bottom_general"
        android:paddingEnd="@dimen/padding_end_general"
        android:paddingLeft="@dimen/padding_start_general"
        android:paddingRight="@dimen/padding_end_general"
        android:paddingStart="@dimen/padding_start_general"
        android:paddingTop="@dimen/padding_top_general"
        android:text="@string/send_a_question"
        android:textAllCaps="true"
        android:textSize="@dimen/font_size_general"
        android:textStyle="bold"
        android:textColor="#ff000000"/>

    <TextView
        android:id="@+id/tvContactForm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_bottom_general"
        android:layout_marginEnd="@dimen/margin_end_general"
        android:layout_marginLeft="@dimen/margin_start_general"
        android:layout_marginRight="@dimen/margin_end_general"
        android:layout_marginStart="@dimen/margin_start_general"
        android:layout_marginTop="@dimen/margin_top_general"
        android:paddingBottom="@dimen/padding_bottom_general"
        android:paddingEnd="@dimen/padding_end_general"
        android:paddingLeft="@dimen/padding_start_general"
        android:paddingRight="@dimen/padding_end_general"
        android:paddingStart="@dimen/padding_start_general"
        android:paddingTop="@dimen/padding_top_general"
        android:text="@string/contact_form"
        android:gravity="center_vertical"
        android:textSize="@dimen/font_size_general"
        android:textColor="#ff000000"/>

</LinearLayout>
